#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/discordpy/aoijs/node_modules/.pnpm/jsonrepair@3.8.1/node_modules/jsonrepair/bin/node_modules:/mnt/c/discordpy/aoijs/node_modules/.pnpm/jsonrepair@3.8.1/node_modules/jsonrepair/node_modules:/mnt/c/discordpy/aoijs/node_modules/.pnpm/jsonrepair@3.8.1/node_modules:/mnt/c/discordpy/aoijs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/discordpy/aoijs/node_modules/.pnpm/jsonrepair@3.8.1/node_modules/jsonrepair/bin/node_modules:/mnt/c/discordpy/aoijs/node_modules/.pnpm/jsonrepair@3.8.1/node_modules/jsonrepair/node_modules:/mnt/c/discordpy/aoijs/node_modules/.pnpm/jsonrepair@3.8.1/node_modules:/mnt/c/discordpy/aoijs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cli.js" "$@"
else
  exec node  "$basedir/../../bin/cli.js" "$@"
fi
